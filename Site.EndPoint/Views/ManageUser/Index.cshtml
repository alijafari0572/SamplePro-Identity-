@model List<IndexUser_ViewModel>


<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">UserName</th>
            <th scope="col">Email</th>
            <th scope="col">Claim</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>
                    <a asp-controller="ManageUser" asp-action="AddUserClaim" asp-route-id="@item.Id" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">add</a>
                </td>
            </tr>
        }
    </tbody>
</table>